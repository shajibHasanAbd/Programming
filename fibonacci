#include <stdio.h>
int main(){

    int n1=0,n2=1,next,n;
    printf("enter the number : ");
    next=n1+n2;
    scanf("%d",&n);

    printf("fibonacci%d %d ",n1,n2);

    for(int i=1; i<n ; i++){

        printf("%d ", next );
        n1=n2;
        n2= next;
        next=n1+n2;
        }

    return 0;
}
